{
  "name": "@wljkowa/json-transformer",
  "version": "0.5.0",
  "author": "Wolfgang L. J. Kowarschick",
  "license": "MIT",
  "description": "recursively apply transformers to JSON elements",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wljkowa/wk-json-transform.git"
  },
  "keywords": [
    "json",
    "transform"
  ],
  "main": "./transformer/index.js",
  "files": [
    "transformer/"
  ],
  "scripts": {
    "prebuild": "rm -rf transformer",
    "build": "tsc -p tsconfig.cjs.json",
    "test": "jest -i",
    "test:watch": "jest -i --watch",
    "prejsdoc": "rm -rf docs/@wljkowa",
    "jsdoc": "jsdoc -c jsdoc.json",
    "postjsdocOld": "cp -r node_modules/jsdoc/templates/default/static/* docs/@wljkowa/json-transformer/0.5.0/",
    "prejsdoc:js" : "rm -rf docs_js; rm -rf transformer.commented",
    "jsdoc:js": "tsc -p tsconfig.commented.json && jsdoc -c jsdoc_js.json",
    "prelink": "npm run build",
    "link": "npm link  && npm i && npm link @wljkowa/json-transformer",
    "examples:cjs": "node examples_cjs/examples_cjs.js",
    "preexamples:es6": "webpack --mode production --config webpack.config.examples.js",
    "examples:es6": "node examples_es6/examples_bundle_es6.js"  
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "better-docs": "^2.3.0",
    "jest": "^26.1.0",
    "jest-mock-random": "^1.0.3",
    "jsdoc": "^3.6.4",
    "jsdoc-plugin-intersection": "^1.0.3",
    "ts-jest": "^26.1.1",
    "ts-loader": "^6.2.2",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {}
}
